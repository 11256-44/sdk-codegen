{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "name": "sdk-codegen-all",
  "version": "0.2.0-beta-3",
  "description": "A utility for generating SDKs for various languages from an OpenApi specification",
  "devDependencies": {
    "@looker/eslint-config": "^1.0.16",
    "@looker/prettier-config": "^1.0.15",
    "@openapitools/openapi-generator-cli": "^1.0.2-4.2.0",
    "@types/node-fetch": "^2.3.3",
    "eslint": "^6.8.0",
    "file-type": "^12.3.0",
    "lerna": "^3.20.2",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "8.0.1"
  },
  "resolutions": {
    "set-value": "^2.0.1"
  },
  "scripts": {
    "convert": "ts-node packages/scripts/src/specConvert.ts",
    "legacy": "ts-node packages/scripts/src/legacy.ts",
    "sdk": "ts-node packages/scripts/src/sdkGen.ts",
    "jsonify": "ts-node packages/scripts/src/yamlToJson.ts",
    "view": "npx redoc-cli serve",
    "clean": "yarn clean-dist && rm -rf api/ && rm *.?.?.json && rm *.?.?.oas.json",
    "clean-dist": "lerna run clean",
    "deploy:codegen": "lerna run deploy --scope sdk-codegen -c",
    "deploy:sdk": "lerna run deploy --scope sdk",
    "test": "lerna run test",
    "test:ts": "yarn jest --reporters=default --reporters=jest-junit",
    "test:iphone": "xcodebuild test -project swift/looker/looker.xcodeproj -scheme looker-Package -sdk iphonesimulator -destination 'platform=iOS Simulator,name=iPhone 11,OS=13.4.1' | xcpretty --test --color",
    "test:jest": "DOT_ENV_FILE=.env.test jest",
    "ci:test": "yarn lint && yarn test",
    "build": "lerna run build",
    "bootstrap": "lerna clean -y && lerna bootstrap",
    "lint": "run-p -c lint:*",
    "lint-fix": "run-p -c 'lint:es --fix'",
    "lint:es": "eslint 'packages/**/*.ts{,x}' 'services/**/*.ts{,x}'",
    "lint:tsc": "tsc",
    "prettify": "prettier 'packages/**/*.ts?(x)' 'services/**/*.ts?(x)'",
    "prettify-fix": "prettier --write 'packages/**/*.ts?(x)' 'services/**/*.ts?(x)'",
    "prepare": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/looker-open-source/sdk-codegen.git"
  },
  "keywords": [],
  "author": "Looker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/looker-open-source/sdk-codegen/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "homepage": "https://github.com/looker-open-source/sdk-codegen#readme",
  "dependencies": {
    "@stoplight/spectral": "^5.0.0",
    "@types/blueimp-md5": "^2.7.0",
    "@types/ini": "^1.3.30",
    "@types/jest": "^24.0.13",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.144",
    "@types/mustache": "^0.8.32",
    "@types/node": "^11.13.7",
    "@types/prettier": "^1.18.2",
    "@types/readable-stream": "^2.3.5",
    "@types/request": "^2.48.3",
    "@types/request-promise-native": "^1.0.17",
    "blueimp-md5": "^2.11.0",
    "ini": "^1.3.5",
    "jest": "^24.8.0",
    "jest-junit": "^10.0.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "openapi3-ts": "^1.3.0",
    "prettier": "^1.18.2",
    "readable-stream": "^3.4.0",
    "redoc-cli": "^0.9.5",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "swagger2openapi": "^5.3.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.6.0"
  },
  "prettier": "@looker/prettier-config"
}
